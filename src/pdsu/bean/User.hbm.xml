<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<hibernate-mapping package="pdsu.bean">
		<class name="User" table="t_user">
			<id name="id" column="id" type="java.lang.Integer">
			<generator class="native"></generator>  
			</id>
			<property name="username" type="java.lang.String">
				<column name="username" not-null="true"></column>
			</property>
			<property name="password" type="java.lang.String">
				<column name="password" not-null="true"></column>
			</property>
			<property name="sex" type="java.lang.String">
				<column name="sex" not-null="true"></column>
			</property>
			<property name="age" type="java.lang.Integer">
				<column name="age" not-null="true"></column>
			</property>
			<property name="city" type="java.lang.String">
				<column name="city" not-null="true"></column>
			</property>
			<property name="intersting" type="java.lang.String">
				<column name="intersting" not-null="true"></column>
			</property>
			<property name="status" type="java.lang.Integer">
				<column name="status" not-null="true"></column>
			</property>
		</class>
	</hibernate-mapping>